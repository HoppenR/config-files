!
! ~/.Xresources
!
! Vim: set tabstop=4 :

Xft.dpi:						96
Xft.antialias:					true
Xft.hinting:					true
Xft.rgba:						rgb
Xft.autohint:					false
Xft.hintstyle:					hintfull
Xft.lcdfilter:					lcddefault

Xcursor.theme:					Adwaita
Xcursor.size:					0

XTerm*background:				#000000
XTerm*foreground:				#E8E8E8
XTerm*cursorColor:				#16A085
XTerm*cursorBlink:				true
XTerm*faceName:					Fixed
XTerm*faceSize:					11
XTerm*pointerColor:				#16A085
XTerm*pointerColorBackground:	#2B2C2B
XTerm*reverseVideo:				on
XTerm*selectToClipboard:		true

